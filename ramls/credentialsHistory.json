{
  "title": "Credentials History Schema",
  "description": "Object with information about previously used password",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "userId": {
      "type": "string",
      "description": "User id"
    },
    "hash": {
      "type": "string",
      "description": "Password hash"
    },
    "salt": {
      "type": "string",
      "description": "Password salt"
    },
    "date": {
      "type": "string",
      "format": "date-time",
      "description": "Date when the password was added to history"
    },
    "metadata": {
      "type": "object",
      "$ref": "raml-util/schemas/metadata.schema"
    }
  }
}
